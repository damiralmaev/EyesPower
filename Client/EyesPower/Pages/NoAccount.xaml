<Page x:Class="EyesPower.Pages.NoAccount"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EyesPower.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="421" d:DesignWidth="485"
      Title="NoAccount">

    <Grid>
        <Image Source="pack://application:,,,/Resources/user.png" Height="105" Margin="189,10,187,0" VerticalAlignment="Top"/>
        <Label Content="Вы не вошли в аккаунт!" FontSize="25" HorizontalAlignment="Left" Margin="101,170,0,0" VerticalAlignment="Top"/>
        <Button ToolTip="Если у вас есть аккаунт" x:Name="btinput" Content="Войди" FontSize="25" HorizontalAlignment="Left" Margin="197,232,0,0" VerticalAlignment="Top" Width="97" Height="61" Click="btinput_Click"/>
        <Button ToolTip="Если у вас нет аккаунта" x:Name="btnew" Content="Зарегистрироваться" FontSize="25" HorizontalAlignment="Left" Margin="121,304,0,0" VerticalAlignment="Top" Width="260" Height="61" Click="btnew_Click"/>
    </Grid>
</Page>
