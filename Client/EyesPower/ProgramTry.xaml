<Window x:Name="___main" x:Class="EyesPower.ProgramTry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EyesPower"
        mc:Ignorable="d"
        Title="EyesPower: Программы исключение" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Height="455.248" Width="436.589" Closing="___main_Closing">
    <Grid>
        <Button ToolTip="Сохраняет процессы в файл" x:Name="btdone" Content="Применить" FontSize="30" HorizontalAlignment="Left" Margin="120,341,0,0" VerticalAlignment="Top" Width="197" Height="50" Click="btdone_Click"/>
        <Label Content="Выберите процессы" HorizontalAlignment="Left" Margin="160,22,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listprocforward" HorizontalAlignment="Left" Height="233" Margin="10,66,0,0" VerticalAlignment="Top" Width="185"/>
        <ListBox x:Name="listprocback" HorizontalAlignment="Left" Height="233" Margin="235,66,0,0" VerticalAlignment="Top" Width="185"/>
        <Button ToolTip="Добавить" x:Name="btforward" Content="-->" HorizontalAlignment="Left" Margin="200,82,0,0" VerticalAlignment="Top" Width="30" Height="17" Click="btforward_Click"/>
        <Button ToolTip="Удалить" x:Name="lbback" Content="--" HorizontalAlignment="Left" Margin="200,113,0,0" VerticalAlignment="Top" Width="30" Height="17"/>
        <Button x:Name="btadd" ToolTip="Добавить свой процесс"  HorizontalAlignment="Left" Margin="361,3,0,0" VerticalAlignment="Top" Width="57" Height="51" Click="btadd_Click">
            <StackPanel Orientation=" Vertical">
                <Image Source="Resources\Add.png" Height="36" Width="36" />
            </StackPanel>
        </Button>

    </Grid>
</Window>
